{
    "name": "pece-project/pece-content-api",
    "description": "PECE content API",
    "type": "project",
    "license": "GNU GENERAL PUBLIC LICENSE",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ec-europa/task-runner"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ec-europa/robo-config"
        },
        {
            "type": "package",
            "package": {
                "name": "nyxtechnology/media_attribution",
                "version": "1.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/nyxtechnology/media_attribution.git",
                    "type": "git",
                    "reference": "8.x-1.x"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "revax-hub/scholarly_lite",
                "version": "1.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/revax-hub/scholarly_lite.git",
                    "type": "git",
                    "reference": "9.x-1.x"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/installers": "^v1.9",
        "consolidation/robo": "^3",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^3.3",
        "drupal/advanced_text_formatter": "^2.1",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/bibcite": "^2.0.0",
        "drupal/bibcite_crossref": "^2.0.0",
        "drupal/bibcite_migrate": "^2.0.0",
        "drupal/blocktabs": "^1.0@beta",
        "drupal/bootstrap_layout_builder": "^2.0",
        "drupal/bricks": "^2.0",
        "drupal/cer": "^4.0@alpha",
        "drupal/color_field": "^2.4",
        "drupal/components": "^2.4",
        "drupal/config_filter": "^2.2",
        "drupal/config_partial_export": "^1.4",
        "drupal/config_split": "^2.0.0-beta4",
        "drupal/config_sync": "^2.0@beta",
        "drupal/config_update": "^1.7",
        "drupal/console": "^1.0.2",
        "drupal/context": "^4.1",
        "drupal/core": "^9.3",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/ctools": "^3.11-rc1",
        "drupal/dashboards": "^2.1@beta",
        "drupal/default_content": "^2.0@alpha",
        "drupal/eck": "^1.0@beta",
        "drupal/elasticsearch_connector": "^7.0@alpha",
        "drupal/field_formatter_range": "^1.4",
        "drupal/field_group": "^3.0",
        "drupal/geocoder": "^3.31",
        "drupal/geofield": "^1.3",
        "drupal/geolocation": "^3.2",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_login": "^1.3",
        "drupal/graphql": "4.x-dev",
        "drupal/inline_entity_form": "^1.0-rc8",
        "drupal/languagefield": "1.x-dev",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/layout_builder_modal": "^1.1",
        "drupal/layout_builder_operation_link": "^2.1",
        "drupal/layout_library": "^1.0@beta",
        "drupal/lb_tabs": "^3.0",
        "drupal/lbl": "^1.0",
        "drupal/licenses_vocabulary": "^2.0@alpha",
        "drupal/location_migration": "^1.0@beta",
        "drupal/m4032404": "^1.0@alpha",
        "drupal/matomo": "^1.11",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/menu_block": "^1.10",
        "drupal/metatag": "^1.10",
        "drupal/migrate_tools": "^5.0",
        "drupal/page_sections": "1.x-dev",
        "drupal/paragraphs": "^1.9",
        "drupal/pathauto": "^1.4",
        "drupal/pbf": "^1.0",
        "drupal/piwik_noscript": "^1.5",
        "drupal/profile": "^1.2",
        "drupal/radix": "^4.10",
        "drupal/redirect": "^1.6",
        "drupal/rest_password": "^8.1",
        "drupal/restui": "^1.17",
        "drupal/rules": "3.x-dev",
        "drupal/search_api": "^1.28",
        "drupal/simple_oauth": "^5.0",
        "drupal/single_content_sync": "^1.3",
        "drupal/slick": "^2.6",
        "drupal/slick_views": "^2.6",
        "drupal/smart_trim": "^2.0",
        "drupal/tagclouds": "^2.0",
        "drupal/text_field_formatter": "^2.0",
        "drupal/ui_patterns": "^1.2",
        "drupal/ui_patterns_field_formatters": "^1.7",
        "drupal/ui_patterns_layout_builder": "^1.1",
        "drupal/user_restrictions": "^2.0",
        "drupal/vartheme_bs5": "^1.0@RC",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/workflow": "^1.4",
        "drush/drush": "^10.3",
        "nyxtechnology/media_attribution": "1.0",
        "oomphinc/composer-installers-extender": "~1.0 || ~2.0",
        "revax-hub/scholarly_lite": "1.0",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/config_devel": "^1.8",
        "drupal/core-dev": "^9.3",
        "drupal/devel": "^4.0",
        "drupal/drupal-driver": "^2.1.1",
        "drupal/drupalmoduleupgrader": "^1.9",
        "drupal/migrate_devel": "2.0-alpha2",
        "ec-europa/toolkit": "^9",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/SetupWizard.php"
        ],
        "files": ["load.environment.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEuropa\\Site\\Tests\\": "./tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "Subsite\\composer\\SetupWizard::setup"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/slick": ["npm-asset/slick-carousel"],
            "web/libraries/ace": ["npm-asset/ace-builds"],
            "web/libraries/jquery-ui-touch-punch": ["bower-asset/jqueryui-touch-punch"],
            "web/libraries/swagger-ui/dist": ["npm-asset/swagger-ui-dist"],
            "web/libraries/{$name}": [
              "type:drupal-library",
              "type:bower-asset",
              "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "drupal-libraries": {
            "library-directory": "web/libraries",
            "libraries": [
              {"name": "objectfitpolyfill", "package": "bower-asset/objectfitpolyfill"},
              {"name": "chartjs", "package": "bower-asset/chartjs"},
              {"name": "c3","package": "bower-asset/c3"},
              {"name": "d3", "package": "bower-asset/d3"},
              {"name": "dropzone","package": "npm-asset/dropzone"},
              {"name": "blazy", "package": "npm-asset/blazy"},
              {"name": "slick", "package": "npm-asset/slick-carousel"},
              {"name": "ace", "package": "npm-asset/ace-builds"},
              {"name": "jquery-ui-touch-punch", "package": "bower-asset/jqueryui-touch-punch"},
              {"name": "imagesloaded", "package": "npm-asset/imagesloaded"},
              {"name": "swagger-ui", "package": "npm-asset/swagger-ui-dist"}
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "allowed-packages": [
                "drupal/core",
                "drupal/ui_patterns_layout_builder"
            ],
            "locations": {
              "web-root": "./web"
            }
        },
        "patches": {
            "drupal/core": {
                "Error empty system.site UUID": "https://www.drupal.org/files/issues/2020-03-28/3047392-17_0.patch"
            },
            "drupal/profile" : {
                "2957256 - Migrate data from Profile2": "https://www.drupal.org/files/issues/2020-05-15/2957256-36.patch"
            },
            "drupal/eck": {
                "2815453 - Add migrations for Drupal 7 ECK to Drupal 8/9": "https://www.drupal.org/files/issues/2020-11-13/eck-migrate-d9-2815453-36.patch"
            }
        }
    }
}
